{
  "name": "strava-challenge",
  "version": "1.0.0",
  "description": "Allow users to create private challenges and share them with their friends.",
  "main": "dist/start.js",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "clean": "./scripts/clean.sh",
    "start": "./scripts/start.sh",
    "test": "./scripts/test.sh",
    "compile": "./scripts/compile.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmoseley/strava-challenge.git"
  },
  "keywords": [
    "strava",
    "training",
    "exercise"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/(.*/)*(__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "Jeremy Moseley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmoseley/strava-challenge/issues"
  },
  "homepage": "https://github.com/jmoseley/strava-challenge#readme",
  "dependencies": {
    "@types/convict": "^4.1.1",
    "@types/express": "^4.11.0",
    "@types/express-promise-router": "^2.0.0",
    "@types/jest": "^22.0.0",
    "@types/pug": "^2.0.4",
    "@types/shortid": "0.0.29",
    "@types/winston": "^2.3.7",
    "convict": "^4.0.2",
    "express": "^4.16.2",
    "express-promise-router": "^2.0.0",
    "pug": "^2.0.0-rc.4",
    "shortid": "^2.2.8",
    "typescript": "^2.6.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "forever": "^0.15.3",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0"
  }
}
